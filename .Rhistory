# Chromium (Cr) concentrations
Cr <- c(34, 1, 511, 2, 574, 496, 322, 424, 269, 140, 244, 252, 76, 108, 24, 38, 18, 34, 30, 191)
# Nickel (Ni) concentrations
Ni <- c(23, 22, 55, 39, 283, 34, 159, 37, 61, 34, 163, 140, 32, 23, 54, 837, 64, 354, 376, 471)
# Determine bin widths
Cr_range <- range(Cr)
Ni_range <- range(Ni)
Cr_width <- diff(Cr_range)/10 # Adjust the number 10 as needed for the number of bins
Ni_width <- diff(Ni_range)/10
# Create histograms
hist(Cr, breaks = seq(min(Cr_range), max(Cr_range) + Cr_width, Cr_width),
main = "Histogram of Chromium (Cr) Concentrations",
xlab = "Concentration (mg/kg)", ylab = "Frequency")
hist(Ni, breaks = seq(min(Ni_range), max(Ni_range) + Ni_width, Ni_width),
main = "Histogram of Nickel (Ni) Concentrations",
xlab = "Concentration (mg/kg)", ylab = "Frequency")
# Chromium (Cr) concentrations
Cr <- c(34, 1, 511, 2, 574, 496, 322, 424, 269, 140, 244, 252, 76, 108, 24, 38, 18, 34, 30, 191)
# Nickel (Ni) concentrations
Ni <- c(23, 22, 55, 39, 283, 34, 159, 37, 61, 34, 163, 140, 32, 23, 54, 837, 64, 354, 376, 471)
# Combine the concentrations into a data frame
data <- data.frame(Chromium = Cr, Nickel = Ni)
# Create comparative boxplots
boxplot(data, main = "Comparative Boxplots of Chromium (Cr) and Nickel (Ni) Concentrations",
ylab = "Concentration (mg/kg)", col = c("lightblue", "lightgreen"), names = c("Chromium", "Nickel"))
